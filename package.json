{
  "name": "raar-admin-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production && npm run package",
    "package": "cd dist/raar-admin-ui/browser && gzip -kf *.js *.css && tar czf ../../raar-admin-ui.tar.gz .",
    "lint": "ng lint",
    "format": "prettier --write \"./**/*.{js,ts,json,css,scss,html,md,yaml}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:once": "npm run cy:once:prepare && npm run cy:once:serve_and_run",
    "cy:once:serve_and_run": "run-p --race cy:once:serve cy:run",
    "cy:once:prepare": "npm run build && ln -sf index.html dist/raar-admin-ui/404.html",
    "cy:once:serve": "http-server ./dist/raar-admin-ui -a localhost -p 4200 -c-1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.7",
    "@angular/common": "^20.3.7",
    "@angular/compiler": "^20.3.7",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/platform-browser": "^20.3.7",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.7",
    "bootstrap-sass": "^3.4.1",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.5.0",
    "@angular-eslint/eslint-plugin": "^20.5.0",
    "@angular-eslint/eslint-plugin-template": "^20.5.0",
    "@angular-eslint/schematics": "^20.5.0",
    "@angular-eslint/template-parser": "^20.5.0",
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.7",
    "@angular/language-service": "^20.3.7",
    "@types/node": "^22.18.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "sass": "1.77.6"
  }
}
